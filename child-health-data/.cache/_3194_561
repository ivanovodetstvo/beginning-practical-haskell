<script>

    $(document).ready( function(){
        $('#search-box').on('keypress',function (e) {
            if (e.keyCode == 13) { // 13 = Enter Key
                e.preventDefault();
                SubmitSearch($(this));
                return false;
            }
        });

        $("#search-button").on("click", function (e) {
            e.preventDefault();
            SubmitSearch($("#search-box"));
            return false;
        });
    });

    function SubmitSearch($searchbox)
    {
        var searchTerm = $searchbox.val();
        var data = { term: searchTerm, searchIndex: "" };

        if (searchTerm.length > 0) {

            window.setTimeout(function () {
                $searchbox.css('background-image', 'url(/Content/Icons/wait.gif)');
                $searchbox.val('Running Search ...');

                $.post("/ajax/sitesearch/submit", data, function (data, textStatus) {
                   document.location = "/search-results/";
                });
           }, 10);
        }
    }

</script>
